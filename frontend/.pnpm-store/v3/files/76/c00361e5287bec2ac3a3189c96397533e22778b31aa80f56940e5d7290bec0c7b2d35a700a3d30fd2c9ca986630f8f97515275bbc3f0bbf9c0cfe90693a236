import type { Getter, MaybeGetter } from "../../internal/types.js";
/**
 * Description placeholder
 *
 * @export
 * @class Debounced
 */
export declare class Debounced<T> {
    #private;
    constructor(getter: Getter<T>, wait?: MaybeGetter<number>);
    get current(): T;
    cancel(): void;
    setImmediately(v: T): void;
}
